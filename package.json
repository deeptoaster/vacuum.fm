{
  "name": "vacuum.fm",
  "browserslist": "since 0",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:typescript-sort-keys/recommended",
      "plugin:@typescript-eslint/all",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "airbnb",
      "airbnb/hooks",
      "airbnb-typescript",
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/array-type": [
        "error",
        {
          "default": "generic"
        }
      ],
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "type"
      ],
      "@typescript-eslint/no-confusing-void-expression": [
        "error",
        {
          "ignoreArrowShorthand": true
        }
      ],
      "@typescript-eslint/no-inferrable-types": [
        "error",
        {
          "ignoreParameters": true
        }
      ],
      "@typescript-eslint/no-shadow": [
        "error",
        {
          "ignoreOnInitialization": true
        }
      ],
      "@typescript-eslint/no-unnecessary-condition": [
        "error",
        {
          "allowConstantLoopConditions": true
        }
      ],
      "@typescript-eslint/parameter-properties": [
        "error",
        {
          "prefer": "parameter-property"
        }
      ],
      "@typescript-eslint/strict-boolean-expressions": [
        "error",
        {
          "allowNullableObject": false,
          "allowNumber": false,
          "allowString": false
        }
      ],
      "react/jsx-boolean-value": [
        "error",
        "always"
      ],
      "sort-imports": [
        "error",
        {
          "allowSeparatedGroups": true
        }
      ],
      "@typescript-eslint/max-params": "off",
      "@typescript-eslint/no-floating-promises": "off",
      "@typescript-eslint/no-unsafe-enum-comparison": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/prefer-enum-initializers": "off",
      "@typescript-eslint/prefer-for-of": "off",
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/control-has-associated-label": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "react/jsx-props-no-spreading": "off",
      "react/no-array-index-key": "off",
      "react/no-unescaped-entities": "off",
      "react/no-unused-prop-types": "off",
      "react/require-default-props": "off",
      "no-else-return": "off",
      "no-nested-ternary": "off"
    }
  },
  "postcss": {
    "plugins": {
      "@csstools/postcss-global-data": {
        "files": [
          "./src/defs.css"
        ]
      },
      "autoprefixer": {},
      "postcss-custom-properties": {
        "preserve": false
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "core-js": "^3.35.1",
    "dotenv-webpack": "^8.0.1",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "squiffles-components": "github:deeptoaster/squiffles-components#master"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@csstools/postcss-global-data": "^2.1.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-transition-group": "^4.4.10",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^8.3.0",
    "css-loader": "^5.2.7",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.1.0",
    "eslint-webpack-plugin": "^2.7.0",
    "postcss-custom-properties": "^13.3.4",
    "postcss-loader": "^5.3.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "build": "webpack --env production --mode production",
    "start": "webpack serve --hot --mode development --open"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/deeptoaster/vacuum.fm.git"
  },
  "author": "Deep Toaster",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/deeptoaster/vacuum.fm/issues"
  },
  "homepage": "http://fishbotwilleatyou.com/vacuum/"
}
